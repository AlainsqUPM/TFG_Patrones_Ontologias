    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#interfaceDescription">
