  <owl:ObjectProperty rdf:about="http://purl.org/iot/ontology/fiesta-priv#hasDomainOfInterest">
    <rdfs:label xml:lang="en">has Domain Of Interest</rdfs:label>
    <rdfs:comment>This property is used to classify devices by DomainOfInterest (e.g., blood pressure sensor is used in healthcare).</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#DomainOfInterest"/>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#hasDomainOfInterest"/>
      </owl:Restriction>
    </rdfs:range>
    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/forProperty">
    <skos:example xml:lang="en">For example, from a Sensor to the properties it can observe; from an Actuator to the properties it can act on; from a Deployment to the properties it was installed to observe or act on; from a SystemCapability to the Property the capability is described for.</skos:example>
    <rdfs:label xml:lang="en">for property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:comment xml:lang="en">A relation between some aspect of an entity and a Property.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/ActuatableProperty"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/forProperty"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/actsOnProperty">
    <skos:example xml:lang="en">In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property on which the Actuator acts upon is the state of the window as it changes from being open to being closed. </skos:example>
    <rdfs:label xml:lang="en">acts on property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation between an Actuation and the property of a FeatureOfInterest it is acting upon.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/ActuatableProperty"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/actsOnProperty"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasMetadata">
    <rdfs:label xml:lang="en">Has Metadata</rdfs:label>
    <rdfs:comment xml:lang="en">
        Links any concept with metadata about that concept.
        </rdfs:comment>
    <rdfs:range rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Metadata"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://git.onem2m.org/MAS/BaseOntology/raw/master/base_ontology.owl#hasService">
    <rdfs:label>Has Service</rdfs:label>
    <rdfs:comment>
        The Functionalities of a Device are exposed in the network as Services of the Device
        </rdfs:comment>
    <owl:equivalentProperty>
      <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeObservation"/>
    </owl:equivalentProperty>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/isPropertyOf">
    <rdfs:label xml:lang="en">is property of</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:comment xml:lang="en">Relation between a Property and the entity it belongs to.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
      </owl:Restriction>
    </rdfs:range>
    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/ActuatableProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/observes">
    <rdfs:label xml:lang="en">observes</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation between a Sensor and an ObservableProperty that it is capable of sensing.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/observes"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeByActuator">
    <rdfs:label xml:lang="en">made by actuator</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation linking an Actuation to the Actuator that made that Actuation.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/madeByActuator"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/iot/ontology/fiesta-priv#hasSource">
    <rdfs:label xml:lang="en">has Source</rdfs:label>
    <rdfs:comment xml:lang="en">This property links sensed phenomena to its environmental source.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Source"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#hasSource"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/adaptcentre/openscience/ontologies/consent#permission_given_for_activity">
    <rdfs:label>permission_given_for_activity</rdfs:label>
    <rdfs:comment>provides a relation between a given action (example discovery of sensors in a region) and permissions needed to fulfil the action.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#Action"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#permission_given_for_activity"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#Permission"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/iot/ontology/fiesta-priv#hasSoundSource">
    <rdfs:label xml:lang="en">has Sound Source</rdfs:label>
    <rdfs:comment xml:lang="en">This property links sound to its environmental source.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/iot/ontology/fiesta-priv#hasSource"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/isHostedBy">
    <rdfs:label xml:lang="en">is hosted by</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isHostedBy"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/System"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeObservation">
    <rdfs:label xml:lang="en">made observation</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation between a Sensor and an Observation made by the Sensor.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/madeObservation"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/adaptcentre/openscience/ontologies/consent#consent_given_for">
    <rdfs:label>consent_given_for</rdfs:label>
    <rdfs:comment>provides link between the consent and permisions for which consent is given.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#Permission"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#consent_given_for"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#Consent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://git.onem2m.org/MAS/BaseOntology/raw/master/base_ontology.owl#hasThingProperty">
    <rdfs:label>Has Thing Property</rdfs:label>
    <rdfs:comment>
        A Thing may have properties that can be described by Values
        </rdfs:comment>
    <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/sosa/observes"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#isAssociatedWith">
    <rdfs:label xml:lang="en">Is Associated With</rdfs:label>
    <rdfs:comment>
        Defines the associations between objects and sensors (e.g. A table (object) has an attribute (temperature at the table) which is associated with a sensor (the temperature sensor of the room).
        </rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <owl:onProperty rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#isAssociatedWith"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/observedProperty">
    <rdfs:label xml:lang="en">observed property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation linking an Observation to the property that was observed. The ObservableProperty should be a property of the FeatureOfInterest (linked by hasFeatureOfInterest) of this Observation.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/observedProperty"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/iot/ontology/fiesta-priv#controls">
    <rdfs:label>controls</rdfs:label>
    <rdfs:comment>This property identies who controls what.</rdfs:comment>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://www.w3.org/ns/ssn/Deployment"/>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#controls"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#controls"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#controls"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#controls"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#controls"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <rdfs:domain rdf:resource="https://w3id.org/GDPRtEXT#Controller"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/iot/ontology/fiesta-priv#hasDirection">
    <rdfs:label>has Direction</rdfs:label>
    <rdfs:comment>The observations made by the sensors are affected by the direction of the sensing device. This property allows observations of the sensor to be associated to the Direction concept.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:onClass rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Direction"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#hasDirection"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Result"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/iot/ontology/fiesta-priv#owns">
    <rdfs:label xml:lang="en">owns</rdfs:label>
    <rdfs:comment xml:lang="en">This property identifies who own the resources or observations</rdfs:comment>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#owns"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#owns"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#owns"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <schema:rangeIncludes>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <owl:onProperty rdf:resource="http://purl.org/iot/ontology/fiesta-priv#owns"/>
      </owl:Restriction>
    </schema:rangeIncludes>
    <rdfs:domain rdf:resource="http://purl.org/adaptcentre/openscience/ontologies/consent#ConsentingParty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://git.onem2m.org/MAS/BaseOntology/raw/master/base_ontology.owl#hasMetaData">
    <rdfs:label>Has MetaData</rdfs:label>
    <rdfs:comment>
        A Value of a Thing can have MetaData (like units, precision-ranges).
        </rdfs:comment>
    <owl:equivalentProperty rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#hasUnit"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#exposes">
    <rdfs:label xml:lang="en">Exposes</rdfs:label>
    <rdfs:comment xml:lang="en">
        For service-oriented queries. The inverse of exposedBy.
        </rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
        <owl:onProperty rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#exposes"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/ssn/System"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hosts">
    <rdfs:label xml:lang="en">hosts</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/hosts"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/isHostedBy"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#interfaceType">
    <rdfs:label xml:lang="en">Interface Type</rdfs:label>
    <rdfs:comment xml:lang="en">
        Defines the type of interface of the service endpoint, e.g. RESTful, NGSI-9, MQTT, CoAP etc.
        </rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#radius">
    <rdfs:label xml:lang="en">Radius</rdfs:label>
    <rdfs:comment>
        Specifies the radius of a circle coverage defined by a point -the centre of the circle- and its radius.
        </rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Circle"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#metadataValue">
    <rdfs:label xml:lang="en">Metadata Value</rdfs:label>
    <rdfs:comment>Value of the metadata</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Metadata"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#isOnline">
    <rdfs:label xml:lang="en">is Online</rdfs:label>
    <rdfs:comment xml:lang="en">Identifies if the service is online or not.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#metadataType">
    <rdfs:label xml:lang="en">Metadata Type</rdfs:label>
    <rdfs:comment>
        Defines the type pf the metadata value (e.g. resolution of the sensor).
        </rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Metadata"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/sosa/resultTime">
    <rdfs:label xml:lang="en">result time</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">The result time is the instant of time when the Observation, Actuation or Sampling activity was completed.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
    <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Actuation"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long">
    <rdfs:label>longitude</rdfs:label>
    <rdfs:comment>The WGS84 longitude of a SpatialThing (decimal degrees).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#alt">
    <rdfs:label>altitude</rdfs:label>
    <rdfs:comment>The WGS84 altitude of a SpatialThing (decimal meters 
