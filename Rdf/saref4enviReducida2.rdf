    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location">
        <rdfs:comment xml:lang="en">The relation between something and the point,  or other geometrical thing in space, where it is.  For example, the realtionship between a radio tower and a Point with a given lat and long. Or a relationship between a park and its outline as a closed arc of points, or a road and its location as a arc (a sequence of points). Clearly in practice there will be limit to the accuracy of any such statement, but one would expect an accuracy appropriate for the size of the object and uses such as mapping .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
        <rdfs:label xml:lang="en">location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/isMeasuredIn -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredIn">
        <rdfs:comment xml:lang="en">A relation between a measurement and the unit of measure in which the measurement value is expressed.</rdfs:comment>
        <rdfs:comment xml:lang="en">is measured in</rdfs:comment>
        <rdfs:isDefinedBy>https://saref.etsi.org/core/</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/makesMeasurement -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/makesMeasurement">
        <rdfs:comment xml:lang="en">A relation between a sensor and the measurements it makes. Such measurement will link together the valur of the measurement, its unit of measure and the property to which it relates.</rdfs:comment>
        <rdfs:isDefinedBy>https://saref.etsi.org/core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">makes measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/measuresProperty -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/measuresProperty">
        <rdfs:comment xml:lang="en">A relation between a sensor and the property o properties it can take measurement about.</rdfs:comment>
        <rdfs:isDefinedBy>https://saref.etsi.org/core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">measures property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/relatesToProperty -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToProperty">
        <rdfs:comment xml:lang="en">A relation between a measurement and the property such measurement is about.</rdfs:comment>
        <rdfs:isDefinedBy>https://saref.etsi.org/core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">relates to property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/affectsProperty -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/affectsProperty">
        <rdfs:comment xml:lang="en">A relation between an actuator with the property or properties it can act on</rdfs:comment>
        <rdfs:label xml:lang="en">affects property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/contains -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/contains">
        <owl:inverseOf rdf:resource="https://saref.etsi.org/saref4envi/isContainedIn"/>
        <rdfs:comment xml:lang="en">A relation between a physical object and the physical objects that can be contained in it.</rdfs:comment>
        <rdfs:label xml:lang="en">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/encapsulates -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/encapsulates">
        <owl:inverseOf rdf:resource="https://saref.etsi.org/saref4envi/hasDigitalRepresentation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">A relation between a digital representation and the physical objects it represents.</rdfs:comment>
        <rdfs:label xml:lang="en">encapsulates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasColor -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasColor">
        <rdfs:comment xml:lang="en">A relation between a light emitted and its color.</rdfs:comment>
        <rdfs:label xml:lang="en">has color</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasComponent -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasComponent">
        <owl:inverseOf rdf:resource="https://saref.etsi.org/saref4envi/isComponentOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">A relation between a system and the system in which it might be decomposed.</rdfs:comment>
        <rdfs:label xml:lang="en">has component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasDigitalRepresentation -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasDigitalRepresentation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">A relation between a physical object and the digital representation in which it is encapsulated.</rdfs:comment>
        <rdfs:label xml:lang="en">has digital representation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasFrequencyMeasurement -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasFrequencyMeasurement">
        <rdfs:comment xml:lang="en">The relation between a device and the frequency in which it makes measurements.</rdfs:comment>
        <rdfs:label xml:lang="en">has frequency measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasGeometry -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasGeometry">
        <rdfs:comment xml:lang="en">A relation between a light emitted and the shape it is projected in.</rdfs:comment>
        <rdfs:label xml:lang="en">has geometry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasLightPoint -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasLightPoint">
        <rdfs:comment xml:lang="en">A relation between a lamppost and its light points</rdfs:comment>
        <rdfs:label xml:lang="en">has light point</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasTransmissionPeriod -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/hasTransmissionPeriod">
        <rdfs:comment xml:lang="en">The relation between a device and the period in which it transmit information.</rdfs:comment>
        <rdfs:label xml:lang="en">has transmission period</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/isAccesibleThrough -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/isAccesibleThrough">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The relation between a digital representation and the service which it is accesible through.</rdfs:comment>
        <rdfs:label xml:lang="en">is accesible through</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/isComponentOf -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/isComponentOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">A relation between a system and the system it belongs to as a component.</rdfs:comment>
        <rdfs:label xml:lang="en">is component of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/isConnectedTo -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/isConnectedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">A relation between a system and the system to which it is connected.</rdfs:comment>
        <rdfs:label xml:lang="en">is connected to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/isContainedIn -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/isContainedIn">
        <rdfs:comment xml:lang="en">A relation between a physical object and the physical object in which it might be contained.</rdfs:comment>
        <rdfs:label xml:lang="en">is contained in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/isProjectedFromHeight -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/isProjectedFromHeight">
        <rdfs:comment xml:lang="en">A relation between a light and the height measurement from which it is projected.</rdfs:comment>
        <rdfs:label xml:lang="en">is projected from height</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/isProjectedInDirection -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/isProjectedInDirection">
        <rdfs:comment xml:lang="en">A relation between a light and the direction in which it is projected.</rdfs:comment>
        <rdfs:label xml:lang="en">is projected in direction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/projectsLight -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/projectsLight">
        <rdfs:comment xml:lang="en">A relation between a lamppost or a light point and the light (or lights) they might project.</rdfs:comment>
        <rdfs:label xml:lang="en">projects light</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/usesCommunicationInterface -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/usesCommunicationInterface">
        <rdfs:comment xml:lang="en">A relation between a system and the communication interface it uses.</rdfs:comment>
        <rdfs:label xml:lang="en">uses communication interface</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/saref4envi/usesCommunicationProtocol -->

    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4envi/usesCommunicationProtocol">
        <rdfs:comment xml:lang="en">A relation between a system and the communication protocol it uses.</rdfs:comment>
        <rdfs:label xml:lang="en">uses communication protocol</rdfs:label>
    </owl:ObjectProperty>
    


