        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">A relationship stating whether a lamppost has a shield.</rdfs:comment>
        <rdfs:label xml:lang="en">has shield</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasTag -->

