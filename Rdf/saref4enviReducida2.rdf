        <rdfs:label xml:lang="en">has identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasProjectionAngle -->

    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4envi/hasProjectionAngle">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">A relationship defining the projection angle of an entity (e.g., a light).</rdfs:comment>
        <rdfs:label xml:lang="en">has projection angle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasRevisionNumber -->

    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4envi/hasRevisionNumber">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A relationship defining the revision number of a certain entity (e.g., a device)</rdfs:comment>
        <rdfs:label xml:lang="en">has revision number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasShield -->

    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4envi/hasShield">
