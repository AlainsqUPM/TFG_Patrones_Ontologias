        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">A relationship defining the creation date of an entity (e.g., a digital representation).</rdfs:comment>
        <rdfs:label xml:lang="en">has creation date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasFlash -->

    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4envi/hasFlash">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">A relationship stating whether a light has flash.</rdfs:comment>
        <rdfs:label xml:lang="en">has flash</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4envi/hasIdentifier -->

    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4envi/hasIdentifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
