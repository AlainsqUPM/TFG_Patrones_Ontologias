        <rdfs:range rdf:resource="http://semanticweb.cs.vu.nl/2009/11/sem/Actor"/>
        <rdfs:comment xml:lang="en">Used to state the victim(s) of the crime event.</rdfs:comment>
        <rdfs:label xml:lang="en">hasVictim</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/CEMontology/hasWeapon -->


    <owl:ObjectProperty rdf:about="https://w3id.org/CEMontology/hasWeapon">
