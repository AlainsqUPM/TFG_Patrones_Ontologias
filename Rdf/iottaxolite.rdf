    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Ionising Radiation</rdfs:label>
    <rdfs:comment xml:lang="en">Ionising radiation is radiation that carries enough energy to free electrons from atoms or molecules, thereby ionizing them. Gamma rays, X-rays, and the higher ultraviolet part of the electromagnetic spectrum are ionizing, whereas the lower ultraviolet part of the electromagnetic spectrum, and also the lower part of the spectrum below UV, including visible light (including nearly all types of laser light), infrared, microwaves, and radio waves are all considered non-ionizing radiation.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3.org/ns/sosa/ObservableProperty"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#PeopleCountSensor">
    <rdfs:label xml:lang="en">People Count Sensor</rdfs:label>
    <rdfs:comment xml:lang="en">Device used to count the number of people (eg., used within an indoor area).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Counter"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#H2SSensor">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">H2S Sensor</rdfs:label>
    <rdfs:comment xml:lang="en">Device used to measure of hydrogen sulphide in the environment.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#GaseousPollutantSensor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#Lavatory">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Lavatory Actuating Device</rdfs:label>
    <rdfs:comment xml:lang="en">An actuator to automatically switch on/off the lavatory.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#NH3Sensor">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">NH3 Sensor</rdfs:label>
    <rdfs:comment xml:lang="en">Device used to detect NH3 level in the environment.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#GaseousPollutantSensor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#VehiclePresenceDetector">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en"> Vehicle Presence Detector</rdfs:label>
    <rdfs:comment xml:lang="en">Device used to detect if an vehicle is present at a place.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#PresenceDetector"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#Salinity">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Salinity</rdfs:label>
    <rdfs:comment xml:lang="en">It is the measure of all the salts dissolved in water.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3.org/ns/sosa/ObservableProperty"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#Thermometer">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:seeAlso rdf:resource="http://purl.oclc.org/NET/ssnx/meteo/aws#TemperatureSensor"/>
    <rdfs:seeAlso rdf:resource="http://casas.wsu.edu/owl/cose.owl#TempuratureSensor"/>
    <rdfs:label xml:lang="en">Thermometer</rdfs:label>
    <rdfs:comment xml:lang="en">A device to measure the temperature in a room or outside.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/sosa/ObservableProperty">
    <skos:example xml:lang="en">The height of a tree, the depth of a water body, or the temperature of a surface are examples of observable properties, while the value of a classic car is not (directly) observable but asserted.</skos:example>
    <rdfs:label xml:lang="en">Observable Property</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">An observable quality (property, characteristic) of a FeatureOfInterest.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/Property"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#RetERabDrop">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">ERAB Drop</rdfs:label>
    <rdfs:comment xml:lang="en">ERAB Drop.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Communication"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#AccRrcConnSetupSuccRate">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">RRC Connection Setup Success Rate</rdfs:label>
    <rdfs:comment xml:lang="en">Radio Resource Control connection setup success rate.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Communication"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#Ampere">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:seeAlso>http://purl.oclc.org/NET/ssnx/qu/unit#ampere</rdfs:seeAlso>
    <rdfs:seeAlso rdf:resource="http://sweet.jpl.nasa.gov/ontology/units.owl#ampere"/>
    <rdfs:seeAlso rdf:resource="http://data.qudt.org/qudt/owl/1.0.0/unit.owl#Ampere"/>
    <rdfs:label xml:lang="en">Ampere (A)</rdfs:label>
    <rdfs:comment xml:lang="en">The ampere is the SI unit for measuring an electric current which is the flow of electric charges through a surface at the rate of one coulomb per second.The ampere is the SI unit for measuring an electric current which is the flow of electric charges through a surface at the rate of one coulomb per second.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/qu/qu#Unit"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#FillLevel">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:comment xml:lang="en">Property used to determine the level a certain object such as waste bin is filled upto.</rdfs:comment>
    <rdfs:label xml:lang="en">Fill Level</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#Lamp">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Lamp Actuating Device</rdfs:label>
    <rdfs:comment xml:lang="en">An actuator to automatically switch on/off the lamp.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Actuator"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#Gauss">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Gauss</rdfs:label>
    <rdfs:comment xml:lang="en">Gauss is the CGS unit of measurement of magnetic flux density (or magnetic induction) (B) (Source Wikipedia)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/qu/qu#Unit"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#SoilTemperature">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Soil Temperature</rdfs:label>
    <rdfs:comment xml:lang="en">Soil temperature is the bulk temperature of the soil, not the surface (skin) temperature.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Temperature"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/sosa/FeatureOfInterest">
    <skos:example xml:lang="en">When measuring the height of a tree, the height is the observed ObservableProperty, 20m may be the Result of the Observation, and the tree is the FeatureOfInterest. A window is a FeatureOfInterest for an automatic window control Actuator.</skos:example>
    <rdfs:label xml:lang="en">Feature Of Interest</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
    <rdfs:comment xml:lang="en">The thing whose property is being estimated or calculated in the course of an Observation to arrive at a Result or whose property is being manipulated by an Actuator, or which is being sampled or transformed in an act of Sampling.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#WaterLevel">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Water Level</rdfs:label>
    <rdfs:comment xml:lang="en">The height reached by the water in a reservoir, river, storage tank, or similar.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#KilogramPerCubicMetre">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:comment xml:lang="en">It is SI derived unit of density, defined by mass in kilograms divided by volume in cubic metres.</rdfs:comment>
    <rdfs:label xml:lang="en">Kilogram Per Cubic Metre, Kilogram Per Cubic Meter</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#GramPerCubicMetre"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#GaseousPollutantSensor">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Gaseous Pollutant Sensor</rdfs:label>
    <rdfs:comment xml:lang="en">Device used to detect poisonous gaseous in the environment.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#AirPollutantSensor"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#PresenceStateEmergencyVehicle">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Presence State Emergency Vehicle</rdfs:label>
    <rdfs:comment xml:lang="en">The presence or absence of an emergency vehicle (ambulance, fire fighters, etc.).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Presence"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#GramPerLitre">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Gram Per Litre, Gram Per Liter (g/L)</rdfs:label>
    <rdfs:comment xml:lang="en">It shows how many grams of a certain substance are present in one litre of a usually liquid or gaseous mixture.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/qu/qu#Unit"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#BodyThermometer">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Body Thermometer</rdfs:label>
    <rdfs:comment xml:lang="en">Device used to measure body temperature.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#Thermometer"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#ConstructionWork">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Construction Work Sound Source</rdfs:label>
    <rdfs:comment xml:lang="en">When the source of the sound was a construction work.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/iot/vocab/iot-taxonomy-lite#SoundSource"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/iot/vocab/iot-taxonomy-lite#BloodCoagulation">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/iot/vocab/m3-lite#"/>
    <rdfs:label xml:lang="en">Blood Coagulation</rdfs:label>
    <dc:description xml:lang="en">Blood Coagulation is the process by which blood changes from  liquid to gel to form a blood clot. It allows the cessation of blood loss from an injury.</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
  </owl:Class>
